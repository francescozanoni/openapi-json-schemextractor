{
  "name": "openapi-json-schemextractor",
  "description": "Extract standard JSON schemas from OpenAPI definitions",
  "version": "1.1.5",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "browser": "dist/openapi-json-schemextractor.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/francescozanoni/openapi-json-schemextractor.git"
  },
  "keywords": [
    "openapi",
    "swagger",
    "json-schema",
    "extractor",
    "converter"
  ],
  "author": "Francesco Zanoni <francescozanoni81@gmail.com>",
  "license": "MIT",
  "files": [
    "/lib",
    "/bin",
    "/dist"
  ],
  "scripts": {
    "test": "jest",
    "style:check": "standard --env jest \"*.js\" \"tests/*.js\" \"lib/*-handler.js\" \"lib/*-manager.js\" \"lib/helpers.js\" \"bin/*.js\"",
    "style:fix": "npm run style:check -- --fix",
    "build": "npx webpack"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.1",
    "@openapi-contrib/openapi-schema-to-json-schema": "^3.0.0",
    "js-yaml": "^3.14.0",
    "json-schema-merge-allof": "^0.7.0"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "jest": "^26.0.1",
    "nock": "^12.0.3",
    "oas-schemas": "https://github.com/OAI/OpenAPI-Specification",
    "standard": "^14.3.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
